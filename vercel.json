{
  "buildCommand": "echo 'Static site - no build required'",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/tryon", "destination": "/pages/tryon/index.html" },
    { "source": "/cart", "destination": "/pages/cart/index.html" },
    { "source": "/pages/(.*)", "destination": "/pages/$1" },
    { "source": "/vercel-deployment/(.*)", "destination": "/vercel-deployment/$1" },
    { "source": "/shared/(.*)", "destination": "/vercel-deployment/shared/$1" },
    { "source": "/ai/(.*)", "destination": "https://ai.reweave.shop/$1" }
  ]
}