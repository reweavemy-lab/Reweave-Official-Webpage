{
  "name": "reweave-batik-luxe",
  "version": "1.0.0",
  "description": "Reweave's official website for the Batik Luxe drop - sustainable fashion that connects culture, community, and environmental responsibility",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=/index.html",
    "build": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --watch",
    "build:prod": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "start": "npm run dev",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.{js,jsx,css,html}",
    "test": "jest",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "sustainable-fashion",
    "batik",
    "luxury",
    "e-commerce",
    "ai-try-on",
    "community",
    "impact",
    "cultural-heritage"
  ],
  "author": "Reweave Team",
  "license": "PROPRIETARY",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reweave/batik-luxe-website.git"
  },
  "bugs": {
    "url": "https://github.com/reweave/batik-luxe-website/issues"
  },
  "homepage": "https://reweave.com"
}
